{
    "compilerOptions": {
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "baseUrl": "src",
      "paths": {
        "src/*": ["./*"]
      }
    },
    "include": ["src/**/*", "src/types.d.ts"],
    "exclude": ["node_modules", "dist"]
  }
  